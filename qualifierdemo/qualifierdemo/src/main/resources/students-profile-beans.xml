<?xml version = "1.0" encoding = "UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
   http://www.springframework.org/schema/context
   http://www.springframework.org/schema/context/spring-context-3.0.xsd">

    <context:annotation-config/>

    <bean id="student1" class="com.example.qualifier.demo.student_profiles.Student">
        <property name="name" value="Yogesh"/>
        <property name="age" value="32"/>
    </bean>

    <bean id="student2" class="com.example.qualifier.demo.student_profiles.Student">
        <property name="name" value="Gayatri"/>
        <property name="age" value="27"/>
    </bean>

    <bean id="profile" class="com.example.qualifier.demo.student_profiles.Profile">
    </bean>

</beans>